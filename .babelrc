{
  "exclude": ["node_modules/@babel/**"],
  "presets": [
    [
      {
        "exclude": [
          "@babel/plugin-transform-regenerator"
        ]
      }
    ]
  ],
  "plugins": [
  "@babel/plugin-transform-named-capturing-groups-regex",
  "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-transform-runtime",
      {
      "regenerator": false,
      "useESModules": true
      }
    ]
  ]
}